{  
  "manifest_version": 2,
  "name": "LbAutoCommit author: 李兵",
  "version": "2.0",
  "description": "Automatic doing.",
  "background": {
    "page":"background.html"},
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "*://*.google.com/",
    "background",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "img/alien.png",
    "default_title": "Auto task",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://ekp.joyu.com/*"],
      "js": ["js/jquery-1.9.1.min.js", "js/lbmars.js", "js/lvmamaWkTime.js"],
      "css": ["css/lvmamaStyle.css", "css/bootstrap.min.css", "css/bs/grid.css"],
      "run_at": "document_start"
    }
  ]
}