{  
  "manifest_version": 2,
  "name": "LbAutoCommit",
  "version": "1.0",
  "description": "Automatic doing.",
  "background": { "scripts": ["js/jquery-1.9.1.min.js", "js/background.js"] },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "background",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "img/alien.png",
    "default_title": "Auto task",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/jquery-1.9.1.min.js", "js/lbmars.js"],
      "run_at": "document_start"
    }
  ]
}